<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.teddy.yuksimakinfo">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcherxhdpi"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_roundhdpi"
        android:supportsRtl="true"
        android:theme="@style/Theme.YukSimakInfo"
        android:name=".ApplicationClass">

        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MainActivity" />
        <activity android:name=".BeritaActivity"/>
        <activity android:name=".AplikasiActivity" />
        <activity android:name=".KontakActivity" />
        <activity android:name=".YSIActivity" />
        <activity android:name=".BisnisActivity" />
        <activity android:name=".MotivasiActivity" />
        <activity android:name=".KulitActivity" />
        <activity android:name=".KesehatanActivity" />
    </application>

</manifest>